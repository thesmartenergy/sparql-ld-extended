BASE <http://example.org/>
PREFIX seas: <https://w3id.org/seas/>
PREFIX sosa: <http://www.w3c.org/ns/sosa/>
PREFIX ssn: <http://www.w3c.org/ns/ssn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sgfn: <http://w3id.org/sparql-generate/fn/>

GENERATE {
  [] a sosa:Observation ;
    sosa:hasFeatureOfInterest <room/2> ;
    sosa:observedProperty <room/1#temperature> ;
    sosa:hasSimpleResult ?result .
}
WHERE {
  BIND( sgfn:JSONPath(?source, "$.t" ) AS ?result )
}
